@page
@using WebAppDETAug2022.Models
@model WebAppDETAug2022.Pages.IPLEventsModel
@{
}

<h1>Welcome To IPL 2022!!</h1>
<h2>Ticket Details </h2>
<table border="1">

    <tr>
        
        <th> Id </th>
        <th> Match </th>
        <th> Price </th>
    </tr>
@foreach (Ticket t in Model.Ticket)
{
    <tr>
        


            <td> @t.Id </td>

            <td> @t.Match </td>

            <td> @t.Price </td>
        
 

    </tr>
}
</table>